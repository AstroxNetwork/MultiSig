{
  "name": "multisig",
  "version": "0.0.1",
  "description": "multisig",
  "private": true,
  "scripts": {
    "script:fixtures": "swc ./clients/fixtures -d dist",
    "script:bootstrap": "swc ./scripts -d dist",
    "pre": "pnpm script:bootstrap && node ./dist/ic_deployer.js --clean",
    "bootstrap": "pnpm run clean && pnpm run create",
    "build": "node ./dist/builder.js",
    "clean": "node ./dist/deployer.js --clean",
    "create": "node ./dist/deployer.js --create",
    "install": "node ./dist/deployer.js --install",
    "reinstall": "node ./dist/deployer.js --reinstall",
    "upgrade": "node ./dist/deployer.js --upgrade",
    "post_install": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --verbose --testPathIgnorePatterns=/scripts/ --testPathPattern=/post_install/ -t",
    "ms:frontend": "cd ./clients/ego_dev_frontend && pnpm install && pnpm dev",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --testPathIgnorePatterns=/post_install/ --testPathPattern=/ego_dev/",
    "script": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --verbose --testPathIgnorePatterns=/post_install/ --testPathPattern=/scripts/ -t",
    "format": "prettier --write {scripts,clients}/**/*.{js,ts,less,json}",
    "format:json": "prettier --write artifacts/**/*.json",
    "format:idl": "prettier --write clients/idls/**/*.ts",

    "deploy:local": "pnpm pre && pnpm bootstrap && pnpm build && pnpm install",

    "deploy:ego": "pnpm script:bootstrap && pnpm bootstrap && pnpm run build && pnpm run install",
    "publish:ego": "pnpm post_install ms_provider && pnpm post_install ms_controller && pnpm post_install btc_wallet",

    "ms:create:live": "cross-env NODE_ENV=production node ./dist/deployer.js --create",
    "ms:install:live": "cross-env NODE_ENV=production node ./dist/deployer.js --install",
    "ms:reinstall:live": "cross-env NODE_ENV=production node ./dist/deployer.js --reinstall",
    "ms:upgrade:live": "cross-env NODE_ENV=production node ./dist/deployer.js --upgrade",
    "ms:script:live": "cross-env NODE_ENV=production NODE_OPTIONS=--experimental-vm-modules jest --verbose --testPathIgnorePatterns=/post_install/ --testPathPattern=/scripts/ -t",
    "ms:post_install:project:live": "cross-env NODE_ENV=production NODE_OPTIONS=--experimental-vm-modules jest --verbose -t",
    "ms:deploy": "pnpm ms:pre && pnpm ms:bootstrap && pnpm ms:build && pnpm ms:install && pnpm ms:post_install:project ego_ops",
    "ms:deploy:live": "pnpm ms:create:live && pnpm ms:install:live && pnpm ms:post_install:project:live ego_ops"
  },
  "author": "devs@astrox",
  "license": "ISC",
  "dependencies": {
    "@dfinity/agent": "0.13.1",
    "@dfinity/candid": "0.13.1",
    "@dfinity/identity": "0.13.1",
    "@dfinity/principal": "0.13.1",
    "@ethers-ancillary/bsc": "^0.0.3",
    "@ethersproject/abstract-provider": "^5.7.0",
    "@ethersproject/abstract-signer": "^5.7.0",
    "@ethersproject/address": "^5.7.0",
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/hash": "^5.7.0",
    "@ethersproject/keccak256": "^5.7.0",
    "@ethersproject/logger": "^5.7.0",
    "@ethersproject/properties": "^5.7.0",
    "@ethersproject/signing-key": "^5.7.0",
    "@ethersproject/transactions": "^5.7.0",
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.141",
    "@swc/jest": "^0.2.20",
    "@types/mockjs": "^1.0.6",
    "@types/yargs": "^17.0.10",
    "bip32": "^3.0.1",
    "bip39": "^3.0.4",
    "buffer": "^6.0.3",
    "crc": "^4.1.1",
    "cross-env": "^7.0.3",
    "cross-fetch": "^3.1.5",
    "ethers": "^5.7.0",
    "js-sha256": "^0.9.0",
    "mockjs": "^1.1.0",
    "starkbank-ecdsa": "^1.1.5",
    "tiny-secp256k1": "^2.2.1",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.18",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "shelljs": "^0.8.5",
    "ts-node": "^10.5.0",
    "tsc-alias": "^1.6.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  }
}